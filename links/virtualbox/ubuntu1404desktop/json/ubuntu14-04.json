{
    "variables": {
        "user": "root",
        "password": "engine"
    },
    "builders":
    [
        {
            "vm_name": "Ubuntu14-04desktop",
            "type": "virtualbox-iso",
            "output_directory": "static/result/20171010102129/output/",
            "disk_size": 6144,
            "headless": false,
            "iso_url":"/home/html/iso/ubuntu-14.04-desktop-amd64.iso",
            "iso_checksum": "dccff28314d9ae4ed262cfc6f35e5153",
            "iso_checksum_type": "md5",
            "http_directory": "http",
            "ssh_username": "{{user `user`}}",
            "ssh_password": "{{user `password`}}",
            "ssh_wait_timeout": "90m",
            "shutdown_command": "echo '{{user `password`}}'|sudo -S shutdown -P now",
            "boot_wait": "5s",
            "floppy_files": [
                "static/result/20171010102129/cfg/ubuntu14-04desktop.cfg"
            ],  
            "boot_command": [
                "<esc><esc><enter><wait>",
                "/install/vmlinuz preseed/file=/floppy/ubuntu14-04desktop.cfg ",
                "debian-installer=en_US auto locale=en_US kbd-chooser/method=us ",
                "hostname=localhost ",
                "keyboard-configuration/modelcode=SKIP ",
                "keyboard-configuration/layout=USA ",
                "keyboard-configuration/variant=USA ",
                "passwd/user-fullname={{user `user`}} ",
                "passwd/user-password-again={{user `password`}} ",
                "passwd/user-password={{user `password`}} ",
                "passwd/username={{user `user`}} ",
                "initrd=/install/initrd.gz -- <enter>"
            ],
      "virtualbox_version_file": ".vbox_version",
      "vboxmanage": [
        [ "modifyvm", "{{.Name}}", "--memory", "2048" ],
        [ "modifyvm", "{{.Name}}", "--cpus", "2" ]
      ],
      "guest_os_type": "Ubuntu_64",
      "hard_drive_interface": "sata"
        }
    ]

}
